#include<iostream>
using namespace std;
int leer(char c1[]);
int longitud(char c[]);
int main()
{
	char c1[50];
	leer(c1);
	cout<<longitud(c1);
	
	
	return 0;
}

int longitud(char c1[]){
	int c=0;
	for(;c1[c];c++);
	return c;	
}

int leer(char c1[])
{
	cout<<"Ingresa una palabra: ";
	cin.getline(c1,50,'\n');
}
